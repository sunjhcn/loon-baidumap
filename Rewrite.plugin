#!name=GoogleRewrite
#!desc=谷歌搜索重定向，将google.cn,google.h等重定向至google.com

[URL Rewrite]
#hostname=*.google.com.hk ,*.g.cn,*.google.cn ,*.google.co.jp

^https:\/\/www\.google\.cn https://www.google.com 302
^https:\/\/www\.google\.co\.jp https://www.google.com 302

[MITM]
hostname = *.google.com.hk ,*.g.cn,*.google.cn ,*.google.co.jp
